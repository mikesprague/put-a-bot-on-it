{
  "name": "put-a-bot-on-it",
  "version": "0.1.0",
  "description": "",
  "exports": "./src/index.js",
  "private": "true",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --inspect=0.0.0.0:9229 ./src/index.js ",
    "do-co:up": "docker-compose -f ./docker-compose.dev.yml up",
    "do-co:down": "docker-compose -f ./docker-compose.dev.yml down --rmi 'all'",
    "slash-deploy": "cross-env NODE_ENV=production node ./src/deploy-slash-commands.js",
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "prestart": "npm run slash-deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">= 17.x",
    "npm": ">= 8.x"
  },
  "keywords": [],
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^1.5.0",
    "@nastyox/rando.js": "^2.0.5",
    "axios": "1.3.1",
    "cheerio": "^1.0.0-rc.12",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "discord-api-types": "^0.37.31",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "he": "^1.2.0",
    "node-localstorage": "^2.2.1",
    "openai": "^3.1.0",
    "puppeteer": "^19.6.3",
    "randomcolor": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prettier-eslint": "^15.0.1"
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "commonjs": false,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "import",
      "prettier"
    ],
    "ignorePatterns": [
      "**/.history/*",
      "**/node_modules/*"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {}
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
