{
  "name": "put-a-bot-on-it",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "private": "true",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./index.js",
    "slash-deploy": "cross-env NODE_ENV=production node ./deploy-slash-commands.js",
    "start": "cross-env NODE_ENV=production node ./index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">= 17.x",
    "npm": ">= 8.x",
    "yarn": ">= 1.22.x"
  },
  "keywords": [],
  "author": "Michael Sprague <mikesprague@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "@nastyox/rando.js": "^2.0.5",
    "axios": "^0.26.0",
    "dayjs": "^1.10.7",
    "discord-api-types": "^0.27.2",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "level": "^7.0.1",
    "node-localstorage": "^2.2.1",
    "randomcolor": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "7.16.5",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.2",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": true
        }
      ]
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "14"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "babel",
      "compat",
      "import",
      "prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 11,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {
      "curly": "error",
      "object-curly-newline": "error",
      "semi": "error",
      "prefer-arrow-callback": "warn",
      "arrow-spacing": "error",
      "eqeqeq": "error",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true,
          "allowTemplateLiterals": true
        }
      ],
      "import/no-cycle": [
        "warn",
        {
          "maxDepth": 1
        }
      ],
      "func-names": "off",
      "no-await-in-loop": "off",
      "no-console": "off",
      "no-param-reassign": "warn",
      "no-unused-vars": "warn",
      "import/prefer-default-export": "off",
      "no-restricted-syntax": "off",
      "no-underscore-dangle": "warn",
      "array-callback-return": "warn"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2
  }
}
