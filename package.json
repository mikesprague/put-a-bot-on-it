{
  "name": "put-a-bot-on-it",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "private": "true",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --inspect=0.0.0.0:9229 ./index.js ",
    "doco:up": "docker-compose -f ./docker-compose.dev.yml up",
    "doco:down": "docker-compose -f ./docker-compose.dev.yml down --rmi 'all'",
    "slash-deploy": "cross-env NODE_ENV=production node ./deploy-slash-commands.js",
    "start": "cross-env NODE_ENV=production node ./index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">= 17.x",
    "npm": ">= 8.x"
  },
  "keywords": [],
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "0.15.0",
    "@discordjs/rest": "0.5.0",
    "@nastyox/rando.js": "2.0.5",
    "axios": "0.27.2",
    "cross-env": "7.0.3",
    "dayjs": "1.11.3",
    "discord-api-types": "0.36.0",
    "discord.js": "13.8.1",
    "dotenv": "16.0.1",
    "level": "8.0.0",
    "node-localstorage": "2.2.1",
    "puppeteer": "15.2.0",
    "randomcolor": "0.6.2",
    "syllable": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
